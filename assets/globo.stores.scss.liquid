#cart-store-pickup {
    .picker {
        width: 350px;
        font-size: 16px;
        text-align: left;
        line-height: 1.2;
        color: $color_1;
        position: relative;
        z-index: 10000;
        -webkit-user-select: none;
        -moz-user-select: none;
        -ms-user-select: none;
        user-select: none;
        right: 0;
        left: 0;
        margin: auto;
        bottom: 40px;
    }
    .picker__input {
        cursor: default;
    }
    .picker__input.picker__input--active {
        border-color: $border_color_1;
    }
    .picker__holder {
        width: 100%;
        overflow-y: auto;
        border-radius: 6px !important;
        -webkit-overflow-scrolling: touch;
        right: 0;
        bottom: 0;
        position: absolute;
        background: #ffffff;
        border: 1px solid #aaaaaa;
        border-top-width: 0;
        border-bottom-width: 0;
        border-radius: 0 0 5px 5px;
        box-sizing: border-box;
        min-width: 176px;
        max-width: 350px;
        max-height: 0;
        -ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=0)";
        filter: alpha(opacity=0);
        -moz-opacity: 0;
        opacity: 0;
        -webkit-transform: translateY(-1em) perspective(600px) rotateX(10deg);
        transform: translateY(-1em) perspective(600px) rotateX(10deg);
        transition: -webkit-transform 0.15s ease-out, opacity 0.15s ease-out, max-height 0s 0.15s, border-width 0s 0.15s;
        transition: transform 0.15s ease-out, opacity 0.15s ease-out, max-height 0s 0.15s, border-width 0s 0.15s;
    }
    .picker__frame {
        padding: 1px;
    }
    .picker__wrap {
        margin: -1px;
    }
    .picker--opened {
        .picker__holder {
            max-height: 25em;
            -ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=100)";
            filter: alpha(opacity=100);
            -moz-opacity: 1;
            opacity: 1;
            border-top-width: 1px;
            border-bottom-width: 1px;
            -webkit-transform: translateY(0) perspective(600px) rotateX(0);
            transform: translateY(0) perspective(600px) rotateX(0);
            transition: -webkit-transform 0.15s ease-out, opacity 0.15s ease-out, max-height 0s, border-width 0s;
            transition: transform 0.15s ease-out, opacity 0.15s ease-out, max-height 0s, border-width 0s;
            box-shadow: 0 6px 18px 1px rgba(0, 0, 0, 0.12);
        }
    }
    .picker__box {
        padding: 0 1em;
    }
    .picker__header {
        text-align: center;
        position: relative;
        margin-top: .75em;
    }
    .picker__month {
        font-weight: 500;
        display: inline-block;
        margin-left: .25em;
        margin-right: .25em;
    }
    .picker__year {
        font-weight: 500;
        display: inline-block;
        margin-left: .25em;
        margin-right: .25em;
        color: $color_2;
        font-size: .8em;
        font-style: italic;
    }
    .picker__select--month {
        border: 1px solid #b7b7b7;
        height: 2em;
        padding: .5em;
        margin-left: .25em;
        margin-right: .25em;
        width: 35%;
        &:focus {
            border-color: $border_color_1;
        }
    }
    .picker__select--year {
        border: 1px solid #b7b7b7;
        height: 2em;
        padding: .5em;
        margin-left: .25em;
        margin-right: .25em;
        width: 22.5%;
        &:focus {
            border-color: $border_color_1;
        }
    }
    .picker__nav--prev {
        position: absolute;
        padding: .5em 1.25em;
        width: 1em;
        height: 1em;
        box-sizing: content-box;
        top: -0.25em;
        left: -1em;
        padding-right: 1.25em;
        &:before {
            content: " ";
            border-top: .5em solid transparent;
            border-bottom: .5em solid transparent;
            border-right: 0.75em solid #000000;
            width: 0;
            height: 0;
            display: block;
            margin: 0 auto;
        }
        &:hover {
            cursor: pointer;
            color: $color_1;
            background: #b1dcfb;
        }
    }
    .picker__nav--next {
        position: absolute;
        padding: .5em 1.25em;
        width: 1em;
        height: 1em;
        box-sizing: content-box;
        top: -0.25em;
        right: -1em;
        padding-left: 1.25em;
        &:before {
            content: " ";
            border-top: .5em solid transparent;
            border-bottom: .5em solid transparent;
            border-right: 0.75em solid #000000;
            width: 0;
            height: 0;
            display: block;
            margin: 0 auto;
            border-right: 0;
            border-left: 0.75em solid #000000;
        }
        &:hover {
            cursor: pointer;
            color: $color_1;
            background: #b1dcfb;
        }
    }
    .picker__nav--disabled {
        cursor: default;
        background: none;
        border-right-color: $border_right_color_1;
        border-left-color: $border_left_color_1;
        &:hover {
            cursor: default;
            background: none;
            border-right-color: $border_right_color_1;
            border-left-color: $border_left_color_1;
        }
        &:before {
            cursor: default;
            background: none;
            border-right-color: $border_right_color_1;
            border-left-color: $border_left_color_1;
            &:hover {
                cursor: default;
                background: none;
                border-right-color: $border_right_color_1;
                border-left-color: $border_left_color_1;
            }
        }
    }
    .picker__table {
        text-align: center;
        border-collapse: collapse;
        border-spacing: 0;
        table-layout: fixed;
        font-size: inherit;
        width: 100%;
        margin-top: .75em;
        margin-bottom: .5em;
        td {
            margin: 0!important;
            padding: 0!important;
        }
    }
    .picker__weekday {
        width: 14.285714286%;
        font-size: .75em;
        padding-bottom: .25em;
        color: $color_2;
        font-weight: 500;
    }
    .picker__day {
        padding: 8px;
        font-weight: 200;
        border: 1px solid transparent;
    }
    .picker__day--today {
        position: relative;
        &:before {
            content: " ";
            position: absolute;
            top: 2px;
            right: 2px;
            width: 0;
            height: 0;
            border-top: 0.5em solid #0059bc;
            border-left: .5em solid transparent;
        }
    }
    .picker__day--disabled {
        &:before {
            border-top-color: $border_top_color_1;
        }
        background: #f5f5f5 !important;
        border-color: $border_color_2 !important;
        color: $color_3 !important;
        cursor: default !important;
        &:hover {
            background: #f5f5f5 !important;
            border-color: $border_color_2 !important;
            color: $color_3 !important;
            cursor: default !important;
        }
    }
    .picker__day--outfocus {
        color: $color_5;
        &:hover {
            cursor: pointer;
            color: $color_1;
            background: #b1dcfb;
        }
    }
    .picker__day--infocus {
        &:hover {
            cursor: pointer;
            color: $color_1;
            background: #b1dcfb;
        }
    }
    .picker__day--highlighted {
        border-color: $border_color_1;
        &:hover {
            cursor: pointer;
            color: $color_1;
            background: #b1dcfb;
        }
    }
    .picker--focused {
        .picker__day--highlighted {
            cursor: pointer;
            color: $color_1;
            background: #b1dcfb;
        }
        .picker__day--selected {
            background: #0089ec;
            color: $color_4;
        }
        .picker__day--disabled {
            background: #f5f5f5;
            border-color: $border_color_2;
            color: $color_3;
            cursor: default;
        }
        .picker__list-item--highlighted {
            cursor: pointer;
            color: $color_1;
            background: #b1dcfb;
        }

        .picker__list-item {
            color: #000000;
            background: #ffffff;
            &:hover {
                color: $color_1;
                background: #b1dcfb;
            }
        }
        .picker__list-item--selected {
            background: #0089ec;
            color: #ffffff;
            z-index: 10;
        }
        .picker__list-item--disabled {
            background: #f5f5f5;
            border-color: $border_color_2;
            color: $color_3;
            cursor: default;
            border-color: $border_color_3;
            z-index: auto;
        }
    }
    .picker__day--selected {
        background: #0089ec;
        color: $color_4;
        &:hover {
            background: #0089ec;
            color: $color_4;
        }
    }
    .picker__day--highlighted.picker__day--disabled {
        background: #bbbbbb;
        &:hover {
            background: #bbbbbb;
        }
    }
    .picker__footer {
        text-align: center;
    }
    .picker__button--today {
        border: 1px solid #ffffff;
        background: #ffffff;
        font-size: .8em;
        padding: .66em 0;
        font-weight: bold;
        width: 33%;
        display: inline-block;
        vertical-align: bottom;
        &:hover {
            cursor: pointer;
            color: $color_1;
            background: #b1dcfb;
            border-bottom-color: $border_bottom_color_1;
        }
        &:focus {
            background: #b1dcfb;
            border-color: $border_color_1;
            outline: none;
        }
        &:before {
            position: relative;
            display: inline-block;
            height: 0;
            content: " ";
            margin-right: .45em;
            top: -0.05em;
            width: 0;
            border-top: 0.66em solid #0059bc;
            border-left: .66em solid transparent;
        }
    }
    .picker__button--clear {
        border: 1px solid #ffffff;
        background: #ffffff;
        font-size: .8em;
        padding: .66em 0;
        font-weight: bold;
        width: 33%;
        display: inline-block;
        vertical-align: bottom;
        &:hover {
            cursor: pointer;
            color: $color_1;
            background: #b1dcfb;
            border-bottom-color: $border_bottom_color_1;
        }
        &:focus {
            background: #b1dcfb;
            border-color: $border_color_1;
            outline: none;
        }
        &:before {
            position: relative;
            display: inline-block;
            height: 0;
            content: " ";
            margin-right: .45em;
            top: -0.25em;
            width: .66em;
            border-top: 3px solid #ee2200;
        }
    }
    .picker__button--close {
        border: 1px solid #ffffff;
        background: #ffffff;
        font-size: .8em;
        padding: .66em 0;
        font-weight: bold;
        width: 33%;
        display: inline-block;
        vertical-align: bottom;
        &:hover {
            cursor: pointer;
            color: $color_1;
            background: #b1dcfb;
            border-bottom-color: $border_bottom_color_1;
        }
        &:focus {
            background: #b1dcfb;
            border-color: $border_color_1;
            outline: none;
        }
        &:before {
            position: relative;
            display: inline-block;
            height: 0;
            content: "\D7";
            top: -0.1em;
            vertical-align: top;
            font-size: 1.1em;
            margin-right: .35em;
            color: $color_5;
        }
    }
    .picker__button--today[disabled] {
        background: #f5f5f5;
        border-color: $border_color_2;
        color: $color_3;
        cursor: default;
        &:hover {
            background: #f5f5f5;
            border-color: $border_color_2;
            color: $color_3;
            cursor: default;
        }
        &:before {
            border-top-color: $border_top_color_1;
        }
    }
    .picker__list {
        list-style: none;
        padding: 0;
        margin: 0;
    }
    .picker__list-item {
        border-bottom: 1px solid #dddddd;
        border-top: 1px solid #dddddd;
        margin-bottom: -1px;
        position: relative;
        background: #ffffff;
        padding: .75em 1.25em;
        &:hover {
            cursor: pointer;
            color: $color_1;
            background: #b1dcfb;
            border-color: $border_color_1;
            z-index: 10;
        }
    }
    .picker__list-item--highlighted {
        border-color: $border_color_1;
        z-index: 10;
        &:hover {
            cursor: pointer;
            color: $color_1;
            background: #b1dcfb;
        }
    }
    .picker__list-item--selected {
        background: #0089ec;
        color: $color_4;
        z-index: 10;
        &:hover {
            background: #0089ec;
            color: $color_4;
            z-index: 10;
        }
    }
    .picker__list-item--disabled {
        background: #f5f5f5;
        border-color: $border_color_2;
        color: $color_3;
        cursor: default;
        border-color: $border_color_3;
        z-index: auto;
        &:hover {
            background: #f5f5f5;
            border-color: $border_color_2;
            color: $color_3;
            cursor: default;
            border-color: $border_color_3;
            z-index: auto;
        }
    }
    .picker--time {
        .picker__button--clear {
            display: block;
            width: 80%;
            margin: 1em auto 0;
            padding: 1em 1.25em;
            background: none;
            border: 0;
            font-weight: 500;
            font-size: .67em;
            text-align: center;
            text-transform: uppercase;
            color: $color_6;
            &:hover {
                color: $color_1;
                background: #b1dcfb;
                background: #ee2200;
                border-color: $border_color_4;
                cursor: pointer;
                color: $color_4;
                outline: none;
                &:before {
                    color: $color_4;
                    border-color: $border_color_5;
                }
            }
            &:focus {
                color: $color_1;
                background: #b1dcfb;
                background: #ee2200;
                border-color: $border_color_4;
                cursor: pointer;
                color: $color_4;
                outline: none;
                &:before {
                    color: $color_4;
                    border-color: $border_color_5;
                }
            }
            &:before {
                top: -0.25em;
                color: $color_6;
                font-size: 1.25em;
                font-weight: bold;
            }
        }
        min-width: 256px;
        max-width: 320px;
        .picker__holder {
            background: #f2f2f2;
        }
        .picker__box {
            padding: 0;
            position: relative;
        }
    }
}
